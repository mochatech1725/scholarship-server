{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Run Debug",
          "runtimeExecutable": "npm",
          "runtimeArgs": ["run", "dev"],
          "skipFiles": [
              "<node_internals>/**"
          ],
          "env": {
              "NODE_ENV": "development"
          },
          "console": "integratedTerminal",
          "internalConsoleOptions": "neverOpen"
      }
  ]
} 